<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link rel="stylesheet" href="../public/stylesheets/" />
    <style>
      .img {
        width: 20%;
      }
      .div-2 {
        width: 50%;
      }
    </style>
  </head>
  <body>
    <% include partials/header %>
    <main>
      <div>
        <% allarticles.forEach((element) => { %>
        <div class="div-2">
          <a href="/articles/<%=element.id%>  ">
            <img
              class="img"
              src="/uploads/<%=element.imageFile  %> "
              alt="image File"
            />
          </a>
        </div>
        <% }) %>
      </div>
    </main>
  </body>
</html>
